{
  "name"             : "nexusnetsoftgmbh/nexuscli",
  "license"          : "MIT",
  "authors"          : [
    {
      "name" : "Mike Bertram",
      "email": "bertram@nexus-netsoft.com"
    }
  ],
  "require-dev": {
    "xervice/development": "^1.0.0",
    "codeception/codeception": "*",
    "symfony/var-dumper": "*",
    "phpstan/phpstan": "*"
  },
  "require": {
    "php": ">=7.1.0",
    "symfony/finder": "^4.1.1",
    "xervice/core": "^4.0.0",
    "xervice/data-provider": "^2.0.0"
  },
  "autoload": {
    "psr-4": {
      "DataProvider\\": "_Generated/"
    },
    "psr-0": {
      "Nexus\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NexusTest\\": "tests/Nexus/"
    }
  },
  "scripts": {
    "phpstan": "vendor/bin/phpstan analyse src/Nexus --level=7"
  },
  "bin": ["bin/nxscli"]
}